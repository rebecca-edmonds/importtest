{
  "name": "@jupyterlite/retro-application-extension",
  "version": "0.1.0-beta.13",
  "description": "JupyterLite - Retro Application Extension",
  "license": "BSD-3-Clause",
  "author": "JupyterLite Contributors",
  "homepage": "https://github.com/jupyterlite/jupyterlite",
  "repository": {
    "type": "git",
    "url": "https://github.com/jupyterlite/jupyterlite.git"
  },
  "bugs": {
    "url": "https://github.com/jupyterlite/jupyterlite/issues"
  },
  "main": "lib/index.js",
  "files": [
    "lib/*.d.ts",
    "lib/*.js",
    "lib/*.js.map",
    "style/*.css",
    "style/index.js"
  ],
  "directories": {
    "lib": "lib/"
  },
  "scripts": {
    "build": "tsc -b",
    "clean": "rimraf lib && rimraf tsconfig.tsbuildinfo",
    "docs": "typedoc src",
    "prepublishOnly": "npm run build",
    "watch": "tsc -b --watch"
  },
  "sideEffects": [
    "style/*.css",
    "style/index.js"
  ],
  "types": "lib/index.d.ts",
  "dependencies": {
    "@jupyterlab/application": "~3.4.7",
    "@jupyterlab/console": "~3.4.7",
    "@jupyterlab/coreutils": "~5.4.6",
    "@jupyterlab/docmanager": "~3.4.7",
    "@jupyterlab/services": "~6.4.6",
    "@jupyterlite/ui-components": "^0.1.0-beta.13",
    "@retrolab/application": "^0.3.21"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.12.1",
    "@types/jest": "^26.0.10",
    "jest": "^26.4.2",
    "rimraf": "~3.0.0",
    "ts-jest": "^26.3.0",
    "typescript": "~4.5.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "extension": true
  },
  "style": "style/index.css",
  "styleModule": "style/index.js"
}
